{
  "name": "innsbruck-blog",
  "version": "0.4.0",
  "description": "A static blog generator",
  "main": "index.js",
  "scripts": {
    "start": "if [ $(node --version | grep -c 'v[45]') -eq 1 ]; then node --harmony_sloppy_let --harmony_destructuring --harmony index.js; else node index.js; fi",
    "test": "if [ $(node --version | grep -c 'v[45]') -eq 1 ]; then mocha -- --harmony_sloppy_let --harmony_destructuring; else mocha; fi",
    "prebuild-desktop": "sh install.sh",
    "electron": "electron ./desktop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quietshu/innsbruck"
  },
  "author": "Shu Ding <quietshu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quietshu/innsbruck/issues"
  },
  "homepage": "https://github.com/quietshu/innsbruck",
  "dependencies": {
    "co-busboy": "^1.3.1",
    "ejs": "^2.4.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-ejs": "^3.0.0",
    "koa-route": "^2.4.2",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "lowdb": "^0.12.5",
    "marked": "^0.3.5",
    "opener": "^1.4.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron-prebuilt": "^1.1.1",
    "mocha": "*",
    "supertest": "^1.2.0"
  }
}
